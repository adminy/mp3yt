<?xml version='1.0' encoding='utf-8'?>
<widget id="io.marin.mp3ytdl" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>YtMP3</name>
    <description>
       An Mp3 Youtube Downloader in cordova with nodejs integration
    </description>
    <author email="marin.bivol@dcu.ie" href="http://codeismagic.com/contactMe">
        Marin Bivol
    </author>
    <content src="index.html" />
    <!-- <plugin name="cordova-plugin-whitelist" spec="1" /> -->
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <!-- Minimum ios version 10.0 -->
        <!-- <preference name="deployment-target" value="12.0" /> -->
    </platform>
</widget>
